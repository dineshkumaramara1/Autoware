<?xml version="1.0"?>
<package format="2">

  <name>velodyne_decoder</name>
  <version>1.2.0</version>
  <description>
    Point cloud conversions for Velodyne 3D LIDARs.
  </description>
  <maintainer email="priyanka@aurobots.com">Priyanka</maintainer>
  <author>Jack O'Quin</author>
  <author>Piyush Khandelwal</author>
  <author>Jesse Vera</author>
  <author>Ke Sun</author>
  <license>GNC General Public License V3.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>nodelet</depend>
  <depend>pluginlib</depend>
  <depend>roscpp</depend>
  <depend>sensor_msgs</depend>

  <depend>pcl_ros</depend>
  <depend>pcl_conversions</depend>
  <depend>libpcl-all-dev</depend>
  <depend>libpcl-all</depend>

  <depend>velodyne_msgs</depend>

  <export>
    <nodelet plugin="${prefix}/nodelet_velodyne_decoder.xml"/>
  </export>

</package>
